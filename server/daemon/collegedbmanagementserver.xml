<service>
	<id>collegedbmanagementserver.exe</id>
	<name>College DB Management Server</name>
	<description>Node-Windows Service for starting every bootup time</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\user\Desktop\collegedb\app\server\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Users\user\Desktop\collegedb\app\server\app.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>College DB Management Server wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>DESKTOP-AQ8R90I</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\Users\user\Desktop\collegedb\app\server</workingdirectory>
</service>